  "dependencies": {
    "@lasso/marko-taglib": "^1.0.15",
    "fastify": "^2.7.1",
    "fastify-static": "^2.5.0",
    "lasso": "^3.3.1",
    "lasso-marko": "^2.4.7",
    "marko": "^4.18.10",
    "point-of-view": "^3.5.0"
  }
